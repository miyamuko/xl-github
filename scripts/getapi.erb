(defun <%= name %> (<%= arg_list %>&key nomsg)
<% if description and description != "" %>
  "<%= description %>"
<% end %>
<% if args.empty? %>
  (github-get "<%= path_template %>" :nomsg nomsg))
<% else %>
  (github-get "<%= path_template %>"
              :uri-params (list <%= arg_list.strip %>)
              :nomsg nomsg))
<% end %>
